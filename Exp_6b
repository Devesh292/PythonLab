def gen_fibo(n):

	a = 1
	b = 1
	series=[]

	for i in range(n):
		series.append(a)
		a,b = b,a+b
	return series

x = int(input("Enter a number :\n"))

print("Fibonacci Series is:")

for num in gen_fibo(x):
	print(num,end=' ')

